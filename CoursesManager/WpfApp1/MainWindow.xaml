            <Label x:Name="LabelPriceAvto" HorizontalAlignment="Left" Height="45" Margin="106,450,0,0" VerticalAlignment="Top" Width="180" Background="#FFE7D7D7" FontSize="20" Content="3200"/>
            <Label x:Name="LabelID" HorizontalAlignment="Left" Height="45" Margin="350,530,0,0" VerticalAlignment="Top" Width="400" Background="#FFE7D7D7" FontSize="20" Content="ID"/>
