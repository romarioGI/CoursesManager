            <TextBox x:Name="TextBoxName" HorizontalAlignment="Left" Height="45" Margin="868,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="205" Background="#FFE7D7D7" FontSize="20"/>
            <TextBox x:Name="TextBoxSurname" HorizontalAlignment="Left" Height="45" Margin="868,290,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="205" Background="#FFE7D7D7" FontSize="20"/>
            <Label x:Name="LabelSurname" HorizontalAlignment="Left" Height="45" Margin="868,210,0,0" VerticalAlignment="Top" Width="205" Background="#FFE7D7D7" FontSize="20" Content="Surname"/>
            <Label x:Name="LabelName" HorizontalAlignment="Left" Height="45" Margin="868,50,0,0" VerticalAlignment="Top" Width="205" Background="#FFE7D7D7" FontSize="20" Content="Name"/>
            <Button x:Name="ButtonAdminWindow" Content="AdminWindow" HorizontalAlignment="Left" Height="55" Margin="868,361,0,0" VerticalAlignment="Top" Width="205" IsCancel="True" FontSize="20" Click="ButtonAdminWindow_Click" Background="#FF6D8EDA"/>
            <Label x:Name="LabelIsAddClaim" Content="" HorizontalAlignment="Left" Height="45" Margin="106,619,0,0" VerticalAlignment="Top" Width="644" FontSize="20"/>
            <TextBox x:Name="TextBoxIDInto" HorizontalAlignment="Left" Height="45" Margin="868,530,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="205" Background="#FFE7D7D7" FontSize="20"/>
